eqn1 = {D[u[x, t], t] - D[u[x, t], {x, 2}] == 0, u[x, 0] == 0, 
  u[0, t] == t^2*Sin[t], u[20, t] == 0}
sol1 = u[x, t] /. 
  NDSolve[eqn1, u[x, t], {x, 0, 20}, {t, 0, 10}, PrecisionGoal -> 3][[
   1]]
Plot3D[sol1, {x, 0, 20}, {t, 0, 10}, AxesLabel -> {"x", "t", "u"}, 
 Ticks -> {{0, 1, 2, 3, 4}, {0, 1}, {-3, 0}}]


